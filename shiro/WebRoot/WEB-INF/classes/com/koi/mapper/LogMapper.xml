<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.koi.mapper.LogMapper">
	<insert id="addLog" parameterType="log">
		INSERT INTO
		log(operateor,operateType,operateDate,operateResult,ip)VALUES(#{operateor},#{operateType},#{operateDate},#{operateResult},#{ip})
	</insert>
</mapper>
